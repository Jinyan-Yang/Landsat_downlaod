    "%%time\n",
    "resolution = 0.1  ## in degree\n",
    "\n",
    "grid_lon = [x for x in np.arange(-180., 180., resolution)]\n",
    "grid_lat = [y for y in np.arange(-90., 90., resolution)]\n",
    "grid = list(itertools.product(grid_lon, grid_lat))\n",
    "\n",
    "bm = Basemap()\n",
    "def if_land(point):\n",
    "    lon, lat = point\n",
    "    return bm.is_land(lon, lat)\n",
    "\n",
    "with Pool(50) as p:\n",
    "    returned = p.map(if_land, grid)\n",
    "\n",
    "grid_land = []\n",
    "for i, item in enumerate(returned):\n",
    "    if item is True:\n",
    "        grid_land.append(grid[i])\n",
    "\n",
    "print(len(grid_land))"
   ]
