   "cell_type": "code",
   "execution_count": 5,
   "id": "35fc7dbf",
   "metadata": {},
   "outputs": [],
   "source": [
    "class DownloadTS:\n",
    "    def __init__(self, grid_land, out_file):\n",
    "        self.grid_land = grid_land\n",
    "        self.out_file = out_file\n",
    "        self.sensor_list = [\n",
    "            ('LT5', datetime(1984, 3, 16), datetime(2012, 5, 5)),\n",
    "            ('LC8', datetime(2013, 3, 18), datetime(2022, 9, 30))]\n",
    "        self.tier_list = ['T1']\n",
    "        self.number_threads = 30\n",
    "        self.point_per_file = 10000\n",
    "    \n",
    "    def run(self, k):\n",
    "\n",
    "        out_csv = f'{self.out_file}_p{k}.csv'\n",
    "\n",
    "        if os.path.exists(out_csv):\n",
    "            print(f'Bypassing {out_csv} as it has already been processed')\n",
    "\n",
    "        ts_list = []\n",
    "        ind_start = k*self.point_per_file\n",
    "        ind_end = min((k+1)*self.point_per_file, len(grid_land))\n",
    "\n",
    "        with Pool(self.number_threads) as p:\n",
    "            returned = p.map(self.run_one, range(ind_start, ind_end))\n",
    "\n",
    "        for i in range(ind_end - ind_start):\n",
    "            ts_list.append(returned[i])\n",
    "\n",
    "        lon_list = [grid_land[i][0] for i in range(ind_start, ind_end)]\n",
    "        lat_list = [grid_land[i][1] for i in range(ind_start, ind_end)]\n",
    "        df = pd.DataFrame(list(zip(lon_list, lat_list, ts_list)),\n",
    "               columns =['lon', 'lat', 'timeseries'])\n",
    "\n",
    "        df.to_csv(out_csv)\n",
    "        \n",
    "        return True\n",
    "\n",
    "    def run_one(self, i):\n",
    "\n",
    "        try:\n",
    "            lon, lat = self.grid_land[i]\n",
    "            timeseries = self.get_bands(point_location=(lon, lat))\n",
    "            ts_cleaned = [item for item in timeseries if None not in item.values()]\n",
    "        except BaseException as e:\n",
    "            ts_cleaned = []\n",
    "            print(e)\n",
    "\n",
    "        return ts_cleaned\n",
    "    \n",
    "    def get_bands(self, point_location):\n",
    "        lon, lat = point_location\n",
    "\n",
    "        timeseries = []\n",
    "\n",
    "        for sensor, start, end in self.sensor_list:\n",
    "\n",
    "            raw_dict = ts_extract(\n",
    "                lon=lon, lat=lat, sensor=sensor,\n",
    "                start=start, end=end, tiers=self.tier_list)\n",
    "            timeseries.extend(raw_dict)\n",
    "\n",
    "        return timeseries"
   ]
  },
